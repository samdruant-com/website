{
  "name": "samdruant-server",
  "packageManager": "pnpm@8.9.2",
  "engines": {
    "node": ">=18.18.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:clear": "rm -rf dist",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "test": "mocha",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "docker:build": "docker build -t samdruant-server:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env  samdruant-server:latest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.8.3",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.6",
    "@types/chai-as-promised": "^7.1.6",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/express-serve-static-core": "^4.17.39",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mocha": "^10.0.2",
    "@types/morgan": "^1.9.6",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.0",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.50.0",
    "eslint-plugin-mocha": "^10.2.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.7",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}